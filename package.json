{
  "name": "evobot",
  "version": "1.0.0",
  "private": true,
  "description": "Akashi Bot",
  "repository": "github:MariNatsuki/AkashiBot",
  "author": "MariNatsuki <natsukimari6295@gmail.com>",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "start:prod": "npm run build && node ./dist/index.js",
    "start": "ts-node src/index.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only --poll src/index.ts",
    "typecheck": "tsc -p ./tsconfig.json",
    "generate:locales": "node src/scripts/translator.js ./src/locales en.json"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false
    }
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "array-move": "^4.0.0",
    "chatgpt": "^5.2.2",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "i18n": "^0.15.1",
    "ioredis": "^5.3.2",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "pagination.djs": "^4.0.9",
    "string-progressbar": "^1.0.4",
    "translate": "^1.4.1",
    "winston": "^3.8.2",
    "youtube-sr": "~4.3.4"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.6",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.9.0",
    "libsodium-wrappers": "^0.7.11",
    "opusscript": "^0.0.8",
    "sodium-native": "^4.0.1",
    "tweetnacl": "^1.0.3"
  },
  "engines": {
    "node": ">=16.11.0"
  }
}
